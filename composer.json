{
    "name":"danslo/magento-hhvm-module-compatibility",
    "description":"Magento installation with a bunch of composer-installed modules for the purpose of checking their compatibility with HHVM.",
    "license": "OSL-3.0",
    "require": {
        "magento-hackathon/magento-composer-installer":"dev-master",
        
        "fbrnc/aoe_scheduler":"0.3.2",
        "fbrnc/aoe_profiler":"0.3.0",
        "connect20/aw_blog":"1.3.1",
        "nexcess/magento-turpentine":"0.6.0",
        "colinmollenhour/cache-backend-redis":"1.5",
        "madepeople/made-cache": "1.4.7",
        "danslo/hiphop-indexer": "0.1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/danslo/HiphopIndexer.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/magento-hackathon/magento-composer-installer"
        },
        {
            "type": "vcs",
            "url": "https://github.com/nexcess/magento-turpentine"
        },
        {
            "type": "vcs",
            "url": "https://github.com/colinmollenhour/Cm_Cache_Backend_Redis"
        },
        {
            "type": "vcs",
            "url": "https://github.com/madepeople/Made_Cache"
        },
        {
            "type": "composer",
            "url": "http://packages.firegento.com"
        }
    ],
    "extra":{
        "magento-root-dir": ".",
        "magento-force": "override",
        "magento-deploystrategy":"symlink"
    }
}
